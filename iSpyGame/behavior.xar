        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_onSentence(self,sentence_lang):
        sentence_lang = json.loads(sentence_lang)
        sentence = str(sentence_lang[0])
        self.logger.warning("mtranslate sentence:" + sentence)

        self.current_language = str(sentence_lang[1])
        self.logger.warning("mtranslate lang:" + self.current_language)

        self.translated = translate(sentence, to_language=self.current_language).encode('utf8')

        self.translation(json.dumps([self.translated,self.current_language]))

        self.logger.warning("mtranslate translation:" + self.translated)
        self.logger.warning("mtranslate current_language:" + self.current_language)
        pass

    def onInput_onLanguage(self,language):
        self.current_language = language]]>
